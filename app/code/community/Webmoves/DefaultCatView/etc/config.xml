<config>
    <modules>
        <Webmoves_DefaultCatView>
            <version>0.0.1</version>
        </Webmoves_DefaultCatView>
    </modules>
    <global>
        <resources>
            <DefaultCatView_setup>
                <setup>
                    <module>Webmoves_DefaultCatView</module>
                    <class>Mage_Eav_Model_Entity_Setup</class>
                </setup>
                <connection>
                    <use>default_setup</use>
                </connection>
            </DefaultCatView_setup>
        </resources>
        <blocks>
            <catalog>
                <rewrite>
                    <product_list_toolbar>Webmoves_DefaultCatView_Block_Catalog_Product_List_Toolbar</product_list_toolbar>
                </rewrite>
            </catalog>
        </blocks>
        <models>
            <defaultcatview>
                <class>Webmoves_DefaultCatView_Model</class>
            </defaultcatview>
        </models>
    </global>
</config>